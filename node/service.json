{
  "memory": 1024,
  "ttl": 60,
  "timeout": 60,
  "minReplicas": 1,
  "maxReplicas": 1,
  "workers": 1,
  "routes": {
    "status": {
      "path": "/_v/vtex-catalog-importer/status",
      "public": true
    },
    "cleanCatalog": {
      "path": "/_v/vtex-catalog-importer/cleanCatalog",
      "public": true
    }
  },
  "events": {
    "runImportEvent": {
      "sender": "ssesandbox04.catalog-importer",
      "keys": ["runImport"]
    },
    "runStep": {
      "sender": "ssesandbox04.catalog-importer",
      "keys": ["runStep"]
    }
  }
}
